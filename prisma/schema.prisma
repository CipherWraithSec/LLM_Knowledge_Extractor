generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analysis {
  id               Int      @id @default(autoincrement())
  title            String?
  topics           String[]
  sentiment        String
  keywords         String[]
  summary          String
  createdAt        DateTime @default(now())
  confidence_score Float?
  original_text    String?
}
